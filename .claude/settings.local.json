{
  "permissions": {
    "allow": [
      "*",
      "Bash(cd:*)",
      "Skill(frontend-design)",
      "Bash(grep:*)",
      "Bash(npm install:*)",
      "Bash(supabase --version:*)",
      "Bash(psql $DATABASE_URL -c \"\\\\d notes\")",
      "Bash(npm run build:*)",
      "Bash(psql:*)",
      "Bash(npm run dev:*)"
    ]
  },
  "hooks": {
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "afplay /System/Library/Sounds/Funk.aiff"
          }
        ]
      }
    ],
    "Notification": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "afplay /System/Library/Sounds/Purr.aiff"
          }
        ]
      }
    ]
  }
}
